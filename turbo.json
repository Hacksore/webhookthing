{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"],
      "env": ["DATABASE_HOST", "DATABASE_USERNAME", "DATABASE_PASSWORD"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {},
    "postinstall": {}
  }
}
