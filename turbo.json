{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "db-studio": {
      "cache": false
    },
    "db-generate": {
      "cache": true
    },
    "build": {
      "dependsOn": ["^build", "^db-generate"],
      "outputs": [".next/**", "dist/**"],
      "env": ["DATABASE_HOST", "DATABASE_USERNAME", "DATABASE_PASSWORD"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^db-generate"],
      "cache": false
    },
    "dev-cli": {
      "cache": false
    },
    "db-push": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "typecheck": {
      "cache": false
    }
  }
}
